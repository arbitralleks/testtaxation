{
    "name": "arbitralleks/testtaxation",
    "type": "library",
    "description": "Flexible taxation system for PHP ecommerce applications.",
    "keywords": [
        "shop",
        "ecommerce",
        "tax",
        "taxes",
        "taxation",
        "vat",
        "sylius"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Paweł Jędrzejewski",
            "homepage": "https://pjedrzejewski.com"
        },
        {
            "name": "Sylius project",
            "homepage": "https://sylius.com"
        }

    ],
    "require": {
        "php": "^7.4 || ^8.0",
        "sylius/registry": "^1.5",
        "sylius/resource": "^1.7"
    },
    "require-dev": {
        "phpspec/phpspec": "^7.0"
    },
    "config": {
        "bin-dir": "bin"
    },
    "autoload": {
        "psr-4": {
            "Testtaxation\\Component\\Taxation\\": ""
        }
